import java.util.StringTokenizer;
class Solution {
    public int lengthOfLastWord(String s) {
        StringTokenizer st=new StringTokenizer(s);
        int c=st.countTokens();
        String a="";
        for (int i=0;i<c;i++)
        {
           a=st.nextToken();
        }
        return a.length();
    }
}
